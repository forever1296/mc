#by forever1296
#패키지 import
import requests
from datetime import datetime
from bs4 import BeautifulSoup
import pymysql.cursors
#오늘 날짜
now_all = datetime.now()
now_want = now_all.strftime('%Y%m%d')

#https://sports.news.naver.com/wfootball/record/ajaxHtmlPlayerRecord.nhn?year=2019&month=12&category=epl&date=20191201

    # 해당 url에서 html을 불러옴.
url = requests.get('https://sports.news.naver.com/wfootball/record/ajaxHtmlPlayerRecord.nhn?year=2019&month=12&category=ligue1&date=20191201')
urlt = requests.get('https://sports.news.naver.com/wfootball/record/index.nhn?category=ligue1&tab=team')
urlc = url.content
urlct = urlt.content
html = str(BeautifulSoup(urlc, "html.parser"))
htmlt = str(BeautifulSoup(urlct, "html.parser"))
#datat = str(htmlt.find_all("script", {"type": "text/javascript"}))
rep1 = html.replace(':','')
rep2 = rep1.replace(',','')
rep1t = htmlt.replace(':','')
rep2t = rep1t.replace(',','')

sdata = rep2.split('"')
sdatat = rep2t.split('"')
#print(sdatat)

rdata = list()
rdatat = list()
sdatatsp = list()
i=0
j=1

conn = pymysql.connect(host='localhost',
        user='root',
        password='root',
        db='Sports',
        charset='utf8')

while i < len(sdata):
    if sdata[i] == 'playerName':
        rdata.append(str(j)+'위')
        rdata.append(str(sdata[i+2]))
        j=j+1
    elif sdata[i] =='teamName':
        rdata.append(str(sdata[i+2]))
    elif sdata[i] == 'goals':
        rdata.append(str(sdata[i+3]))
    elif sdata[i] == 'assist':
        rdata.append(str(sdata[i + 1]))
    elif sdata[i] == 'attackPoint':
        rdata.append(str(sdata[i+3]))
    elif sdata[i] == 'shot':
        rdata.append(str(sdata[i+1]))
    elif sdata[i] == 'foul':
        rdata.append(str(sdata[i + 1]))
    elif sdata[i] == 'booking':
        rdata.append(str(sdata[i + 1]))
    elif sdata[i] == 'dismissal':
        rdata.append(str(sdata[i + 1]))
    elif sdata[i] == 'cornerKicks':
        rdata.append(str(sdata[i + 1]))
    elif sdata[i] == 'penaltyKicks':
        rdata.append(str(sdata[i + 1]))
    elif sdata[i] == 'offside':
        rdata.append(str(sdata[i + 1]))
    elif sdata[i] == 'onTargetShot':
        rdata.append(str(sdata[i + 1]))
    elif sdata[i] == 'gameCount':
        rdata.append(str(sdata[i + 1]))
    i=i+1

u=0
k=0
while u<len(rdata)/15:

    cursor = conn.cursor()
    a = rdata[k]
    b = rdata[k+1]
    c = rdata[k+2]
    d = rdata[k+3]
    e = rdata[k+4]
    f = rdata[k+5]
    g = rdata[k + 6]
    aa = rdata[k + 7]
    bb = rdata[k + 8]
    cc = rdata[k + 9]
    dd = rdata[k + 10]
    ee = rdata[k + 11]
    ff = rdata[k + 12]
    gg = rdata[k + 13]
    hh = rdata[k + 14]
    sql = "INSERT INTO ligue1record(a,b,c,d,e,f,g,aa,bb,cc,dd,ee,ff,gg,hh) VALUES ('"+ a +"','"+ b +"','"+ c +"','"+ d +"','"+ e +"','"+ f +"','"+ g +"','"+ aa +"','"+ bb +"','"+ cc +"','"+ dd +"','"+ ee +"','"+ ff +"','"+ gg +"','"+ hh +"')"
    k = k+15
    cursor.execute(sql)
    u = u + 1
#순위 선수이름 소속 득점 도움 공격포인트 슈팅 파울 경고 퇴장 코너킥 패널티킥 오프사이드 유효슈팅 경기수
#print(rdata)

