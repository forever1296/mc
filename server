            rdata.append(str(sdata[i+14]))
            rdata.append(str(sdata[i+6]))
        elif sdata[i] == 'gameStartTime':
            rdata.append(str(sdata[i+2]))
            l=0
        i = i + 1
    #print(rdata)

    #월 변경. 09일 경우에는 10으로 변경/ 해당 안되면 정상적으로 1씩 증가
    if m==9:
        n = 1
        m = 0
    else:
        m = m + 1
    count = count + 1

    j=0
    k=2
    while j<len(rdata)/6:

        cursor = conn.cursor()
        a = rdata[k]
        b = rdata[k+1]
        c = rdata[k+2]
        d = rdata[k+3]
        e = rdata[k+4]
        f = rdata[k+5]
        sql = "INSERT INTO kleague2(a,b,c,d,e,f) VALUES ('"+ a +"','"+ b +"','"+ c +"','"+ d +"','"+ e +"','"+ f +"')"
