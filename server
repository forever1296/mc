            if sdata[i+2] == '20200108f':
                i = i + 1
            else:
                rdata.append(str(sdata[i+2]))
                DATE.append(str(sdata[i+2]))
                p = p+1
                l=1
        elif sdata[i] == 'scheduleList':
            if sdata[i+2] == "empty":
                rdata.append("")
                rdata.append("")
                rdata.append("")
                rdata.append("")
                rdata.append("경기가 없습니다")
                l=0
        elif sdata[i] == 'homeTeamName':
            if l==0:
                rdata.append(DATE[p])
            rdata.append(str(sdata[i+2]))
            rdata.append(str(sdata[i+10]))
            rdata.append(str(sdata[i+14]))
            rdata.append(str(sdata[i+6]))

        elif sdata[i] == 'gameStartTime':
            rdata.append(str(sdata[i+2]))
            l=0
            #각 경기를 '//' 로 구분. 필요 시 수정가능
#           rdata.append('//')



        i = i + 1

    #print(rdata)
    #월 변경. 09일 경우에는 10으로 변경 / 05일 경우에는 epl이 06,07월 경기가 없어서 08로 변경 / 이 두개가 해당 안되면 정상적으로 1씩 증가

    if m==9:
        n = 1
        m = 0
    elif m==5:
        m = 8
    else:
        m = m + 1
    count = count + 1



    j=0
    k=2
    while j<len(rdata)/6:

        cursor = conn.cursor()
        a = rdata[k]
        b = rdata[k+1]
        c = rdata[k+2]
        d = rdata[k+3]
        e = rdata[k+4]
        f = rdata[k+5]
        sql = "INSERT INTO eredivisie(a,b,c,d,e,f) VALUES ('"+ a +"','"+ b +"','"+ c +"','"+ d +"','"+ e +"','"+ f +"')"
        k = k+6
        cursor.execute(sql)
        j = j + 1
conn.commit()
conn.close()

#-*- coding: utf-8 -*-
#by forever1296
#패키지 import
import requests
from datetime import datetime
from bs4 import BeautifulSoup
import pymysql.cursors

#오늘 날짜
now_all = datetime.now()
now_want = now_all.strftime('%Y%m%d')

# n,m = 월
n = 0
m = 2
count = 1

conn = pymysql.connect(host='localhost',
        user='root',
        password='root',
        db='Sports',
        charset='utf8')

#월 01~05 08~12 변경위한 while문
while count!=9:
    # 해당 url에서 html을 불러옴.
    url = requests.get('https://sports.news.naver.com/wfootball/schedule/index.nhn?year=2019&month=' + str(n) + str(m) + '&category=europa&date=20200108')
    urlc = url.content
    html = str(BeautifulSoup(urlc,"html.parser"))
    #저장된 html에서 find_all 함수를 이용하여 원하는 태그유형,이름을 찾음.
    #변수.find("태그명",{"타입":"찾고자하는클래스명"}
    #data = str(html.find_all("script",{"type":"text/javascript"}))

    #저장이 정상적으로 되었는지 확인.
    #print(data)

    #split 구현 "을 기준으로 구분하여 배열로 저장.
    sdata = html.split('"')

    #split 정상적으로 되었는지 확인
    #print(sdata[6])

    #필요한 데이터 -> date / scheduleList / homeTeamName / awayTeamName / homeTeamScore / awayTeamScore / state 등
    i = 0
    p=-1
    l=0
    rdata = list()
    DATE = list()
    #while문을 이용하여 필요한 데이터 저장
    while i<len(sdata):
        if sdata[i] == 'date':
            #웹사이트에서 오늘 날짜 경기는 상단부에 한번 더 출력되어 해당 데이터 제거 / 똑같이 구현하려면 if문 삭제하면 된다.
            if sdata[i+2] == '20200108f':
                i = i + 1
            else:
                rdata.append(str(sdata[i+2]))
                DATE.append(str(sdata[i+2]))
                p = p+1
                l=1
        elif sdata[i] == 'scheduleList':
            if sdata[i+2] == "empty":
                rdata.append("")
                rdata.append("")
                rdata.append("")
                rdata.append("")
                rdata.append("경기가 없습니다")
                l=0
        elif sdata[i] == 'homeTeamName':
            if l==0:
                rdata.append(DATE[p])
            rdata.append(str(sdata[i+2]))
            rdata.append(str(sdata[i+10]))
            rdata.append(str(sdata[i+14]))
            rdata.append(str(sdata[i+6]))

        elif sdata[i] == 'gameStartTime':
            rdata.append(str(sdata[i+2]))
            l=0
            #각 경기를 '//' 로 구분. 필요 시 수정가능
#           rdata.append('//')



        i = i + 1

    #print(rdata)
    #월 변경. 09일 경우에는 10으로 변경 / 05일 경우에는 epl이 06,07월 경기가 없어서 08로 변경 / 이 두개가 해당 안되면 정상적으로 1씩 증가

    if m==9:
        n = 1
        m = 0
    elif m==5:
        m = 9
    else:
        m = m + 1
    count = count + 1



    j=0
    k=2
    while j<len(rdata)/6:

        cursor = conn.cursor()
        a = rdata[k]
        b = rdata[k+1]
        c = rdata[k+2]
        d = rdata[k+3]
        e = rdata[k+4]
        f = rdata[k+5]
        sql = "INSERT INTO europa(a,b,c,d,e,f) VALUES ('"+ a +"','"+ b +"','"+ c +"','"+ d +"','"+ e +"','"+ f +"')"
        k = k+6
        cursor.execute(sql)
        j = j + 1
conn.commit()
conn.close()

#-*- coding: utf-8 -*-
#by forever1296
#패키지 import
import requests
from datetime import datetime
from bs4 import BeautifulSoup
import pymysql.cursors

#오늘 날짜
now_all = datetime.now()
now_want = now_all.strftime('%Y%m%d')

# n,m = 월
n = 0
m = 1
count = 1

conn = pymysql.connect(host='localhost',
        user='root',
        password='root',
        db='Sports',
        charset='utf8')

#월 01~05 08~12 변경위한 while문
while count!=6:
    # 해당 url에서 html을 불러옴.
    url = requests.get('https://sports.news.naver.com/wfootball/schedule/index.nhn?year=2019&month=' + str(n) + str(m) + '&category=facup&date=20200108')
    urlc = url.content
    html = str(BeautifulSoup(urlc,"html.parser"))
    #저장된 html에서 find_all 함수를 이용하여 원하는 태그유형,이름을 찾음.
    #변수.find("태그명",{"타입":"찾고자하는클래스명"}
    #data = str(html.find_all("script",{"type":"text/javascript"}))

    #저장이 정상적으로 되었는지 확인.
    #print(data)

    #split 구현 "을 기준으로 구분하여 배열로 저장.
    sdata = html.split('"')

    #split 정상적으로 되었는지 확인
    #print(sdata[6])

    #필요한 데이터 -> date / scheduleList / homeTeamName / awayTeamName / homeTeamScore / awayTeamScore / state 등
    i = 0
    p=-1
    l=0
    rdata = list()
    DATE = list()
    #while문을 이용하여 필요한 데이터 저장
    while i<len(sdata):
        if sdata[i] == 'date':
            #웹사이트에서 오늘 날짜 경기는 상단부에 한번 더 출력되어 해당 데이터 제거 / 똑같이 구현하려면 if문 삭제하면 된다.
            if sdata[i+2] == '20200108d':
                i = i + 1
            else:
                rdata.append(str(sdata[i+2]))
                DATE.append(str(sdata[i+2]))
                p = p+1
                l=1
        elif sdata[i] == 'scheduleList':
            if sdata[i+2] == "empty":
                rdata.append("")
                rdata.append("")
                rdata.append("")
                rdata.append("")
                rdata.append("경기가 없습니다")
                l=0
        elif sdata[i] == 'homeTeamName':
            if l==0:
                rdata.append(DATE[p])
            rdata.append(str(sdata[i+2]))
            rdata.append(str(sdata[i+10]))
            rdata.append(str(sdata[i+14]))
            rdata.append(str(sdata[i+6]))

        elif sdata[i] == 'gameStartTime':
            rdata.append(str(sdata[i+2]))
            l=0
            #각 경기를 '//' 로 구분. 필요 시 수정가능
#           rdata.append('//')



        i = i + 1

    print(rdata)
    #월 변경. 09일 경우에는 10으로 변경 / 05일 경우에는 epl이 06,07월 경기가 없어서 08로 변경 / 이 두개가 해당 안되면 정상적으로 1씩 증가


    m = m + 1
    count = count + 1



    j=0
    k=2
    while j<len(rdata)/6:

        cursor = conn.cursor()
        a = rdata[k]
        b = rdata[k+1]
        c = rdata[k+2]
        d = rdata[k+3]
        e = rdata[k+4]
        f = rdata[k+5]
        sql = "INSERT INTO facup(a,b,c,d,e,f) VALUES ('"+ a +"','"+ b +"','"+ c +"','"+ d +"','"+ e +"','"+ f +"')"
        k = k+6
        cursor.execute(sql)
        j = j + 1
conn.commit()
conn.close()

#-*- coding: utf-8 -*-
#by forever1296
#패키지 import
import requests
from datetime import datetime
from bs4 import BeautifulSoup
import pymysql.cursors
#오늘 날짜
now_all = datetime.now()
now_want = now_all.strftime('%Y%m%d')

# n,m = 월
n = 0
m = 3
count = 1

conn = pymysql.connect(host='localhost',
        user='root',
        password='root',
        db='Sports',
        charset='utf8')

while count!=9:
    # 해당 url에서 html을 불러옴.
    url = requests.get('https://sports.news.naver.com/kbaseball/schedule/index.nhn?date=20191026&month=' + str(n) + str(m) + '&year=2019&teamCode=')
    urlc = url.content
    html = str(BeautifulSoup(urlc,"html.parser"))
    #저장된 html에서 find_all 함수를 이용하여 원하는 태그유형,이름을 찾음.
    #변수.find("태그명",{"타입":"찾고자하는클래스명"}
    #data = str(html.find_all("div",{"class":"tb_wrap"}))

    #저장이 정상적으로 되었는지 확인.
    #print(data)

    #split 구현 "을 기준으로 구분하여 배열로 저장.
    sdata0 = html.replace('"td_date">','*td_date*')
    sdata1 = sdata0.replace('"td_hour">', '*td_hour*')
    sdata2 = sdata1.replace('"team_lft">', '*team_lft*')
    sdata3 = sdata2.replace('"td_score">', '*td_score*')
    sdata4 = sdata3.replace('"td_none">', '*td_none*')
    sdata5 = sdata4.replace('"team_rgt">', '*team_rgt*')
    sdata6 = sdata5.replace('<strong>', '*')
    sdata7 = sdata6.replace('</strong>', '*')
    sdata8 = sdata7.replace('</span>', '*')
    sdata9 = sdata8.replace('<em>:</em>', '*')
    sdata = sdata9.split("*")

    #split 정상적으로 되었는지 확인
    #print(sdata10)

    #필요한 데이터 -> date / scheduleList / homeTeamName / awayTeamName / homeTeamScore / awayTeamScore / state 등
    i = 0
    p=-1
    l=0
    rdata = list()
    DATE = list()
    #while문을 이용하여 필요한 데이터 저장
    while i<len(sdata):
        if sdata[i] == 'td_date':
            rdata1 = str("2019년 " + sdata[i+2] + "일")
            rdata2 = rdata1.replace(".","월 ")
            rdata.append(str(rdata2))
            DATE.append(str(rdata2))
            p = p + 1
            l = 1
        elif sdata[i] == 'td_hour':
            if sdata[i+1] == '-':
                if l == 0:
                    rdata.append(DATE[p])
                rdata.append("경기가 없습니다")
                rdata.append("")
                rdata.append("")
                rdata.append("")
                rdata.append("")
                l=0
            else:
                if l == 0:
                    rdata.append(DATE[p])
                rdata.append(str(sdata[i+1]))
                l=0
        elif sdata[i] == 'team_lft':
            rdata.append(str(sdata[i+1]))
        elif sdata[i] == 'team_rgt':
            rdata.append(str(sdata[i+1]))
        elif sdata[i] == 'td_score':
            if sdata[i+3] == "team_rgt":
                rdata.append("")
                rdata.append("경기취소")
            else:
                rdata.append(str(sdata[i+1]))
                rdata.append(str(sdata[i+2]))
                l = 0
        i = i + 1
   # print(rdata)

    #월 변경. 09일 경우에는 10으로 변경 / 05일 경우에는 epl이 06,07월 경기가 없어서 08로 변경 / 이 두개가 해당 안되면 정상적으로 1씩 증가
    if m==9:
        n = 1
        m = 0
    else:
        m = m + 1
    count = count + 1

#memo board


    j=0
    k=0
    while j<len(rdata)/6:

        cursor = conn.cursor()
        a = rdata[k]
        b = rdata[k+1]
        c = rdata[k+2]
        d = rdata[k+3]
        e = rdata[k+4]
        f = rdata[k+5]
        sql = "INSERT INTO kbo(a,b,c,d,e,f) VALUES ('"+ a +"','"+ b +"','"+ c +"','"+ d +"','"+ e +"','"+ f +"')"
        k = k+6
        cursor.execute(sql)
        j = j + 1
conn.commit()
conn.close()

