#패키지 import
import requests
from datetime import datetime
from bs4 import BeautifulSoup
import pymysql.cursors

#오늘 날짜
now_all = datetime.now()
now_want = now_all.strftime('%Y%m%d')

# n,m = 월
n = 0
m = 1
count = 1

conn = pymysql.connect(host='localhost',
        user='root',
        password='root',
        db='Sports',
        charset='utf8')

#월 01~05 08~12 변경위한 while문
while count!=11:
    # 해당 url에서 html을 불러옴.
    url = requests.get('https://sports.news.naver.com/wfootball/schedule/index.nhn?year=2019&month=' + str(n) + str(m) + '&category=epl&date=20200108')
    urlc = url.content
    html = str(BeautifulSoup(urlc,"html.parser"))
    #저장된 html에서 find_all 함수를 이용하여 원하는 태그유형,이름을 찾음.
    #변수.find("태그명",{"타입":"찾고자하는클래스명"}
    #data = str(html.find_all("script",{"type":"text/javascript"}))

    #저장이 정상적으로 되었는지 확인.
    #print(data)

    #split 구현 "을 기준으로 구분하여 배열로 저장.
    sdata = html.split('"')

    #split 정상적으로 되었는지 확인
    #print(sdata[6])

    #필요한 데이터 -> date / scheduleList / homeTeamName / awayTeamName / homeTeamScore / awayTeamScore / state 등
    i = 0
    p=-1
    l=0
    rdata = list()
    DATE = list()
    #while문을 이용하여 필요한 데이터 저장
    while i<len(sdata):
        if sdata[i] == 'date':
            if sdata[i+2] == '20200108da':
                i = i + 1
            else:
                rdata.append(str(sdata[i+2]))
                DATE.append(str(sdata[i+2]))
                p = p+1
                l=1
        elif sdata[i] == 'scheduleList':
            if sdata[i+2] == "empty":
                rdata.append("")
                rdata.append("")
                rdata.append("")
                rdata.append("")
                rdata.append("경기가 없습니다")
                l=0
        elif sdata[i] == 'homeTeamName':
            if l==0:
                rdata.append(DATE[p])
            rdata.append(str(sdata[i+2]))
            rdata.append(str(sdata[i+10]))
            rdata.append(str(sdata[i+14]))
            rdata.append(str(sdata[i+6]))

        elif sdata[i] == 'gameStartTime':
            rdata.append(str(sdata[i+2]))
            l=0

        i = i + 1

    print(rdata)
    #월 변경. 09일 경우에는 10으로 변경 / 05일 경우에는 epl이 06,07월 경기가 없어서 08로 변경 / 이 두개가 해당 안되면 정상적으로 1씩 증가

    if m==9:
        n = 1
        m = 0
    elif m==5:
        m = 8
    else:
        m = m + 1
    count = count + 1



    j=0
    k=2
    while j<len(rdata)/6:

        cursor = conn.cursor()
        a = rdata[k]
        b = rdata[k+1]
        c = rdata[k+2]
        d = rdata[k+3]
        e = rdata[k+4]
        f = rdata[k+5]
        sql = "INSERT INTO epl(a,b,c,d,e,f) VALUES ('"+ a +"','"+ b +"','"+ c +"','"+ d +"','"+ e +"','"+ f +"')"
        k = k+6
        cursor.execute(sql)
        j = j + 1
conn.commit()
conn.close()
