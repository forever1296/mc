import pymysql.cursors

conn = pymysql.connect(host='35.175.193.49',
        user='root',
        password='root',
        charset='utf8mb4')

try:
    with conn.cursor() as cursor:
        sql = 'CREATE DATABASE Sports'
        cursor.execute(sql)
    conn.commit()
finally:
    conn.close()
import pymysql.cursors
import sqlite3

conn = pymysql.connect(
        host='localhost',
        user='root',
        password='root',
        db='Sports',
        autocommit=True)

cursor=conn.cursor()

sql='''
    create table test2(
    a int)
'''

cursor.execute(sql)
conn.commit()
conn.close()
#by forever1296
#패키지 import
import requests
from datetime import datetime
from selenium import webdriver
from bs4 import BeautifulSoup
from selenium.webdriver.common.keys import Keys
import time
import pymysql.cursors

#오늘 날짜
now_all = datetime.now()
now_want = now_all.strftime('%Y%m%d')

# 옵션 추가: 브라우저 창을 띄우지 않은 채 1920x1080 사이즈로 브라우저 실행
options = webdriver.ChromeOptions()
options.add_argument('window-size=1920x1080')

driver = webdriver.Chrome(executable_path="C:\\Users\myken\PycharmProjects\\untitled\webdriver\chromedriver.exe",options = options)

url = ('https://www.premierleague.com/players')
driver.get(url)

body = driver.find_element_by_tag_name('body')
for i in range(17):
    body.send_keys(Keys.END)
    time.sleep(2.0)

html = driver.page_source
soup = BeautifulSoup(html, 'html.parser')
#data = soup.select('')

sdata = list()
data = str(soup)
sdata = data.split('"')
#print(sdata)

rdata = list()
rdata2 = list()
rdatat = list()
i=0
l=0
while i<len(sdata):
#while i<200:
    if sdata[i] == 'playerName':
        rdata.append('https:' + str(sdata[i + 2]))
        rdatat.append(rdata[l].replace("overview","stats"))
        l = l+1
    i = i + 1
j=0
while j<len(rdatat):
    url2 = requests.get(rdatat[j])
    urlc2 = url2.content
    html2 = str(BeautifulSoup(urlc2, "html.parser"))
    rep1 = html2.replace('>','"')
    rep2 = rep1.replace('<','"')
    rep3 = rep2.replace('\n','"')
    rep4 = rep3.replace(' ', '')
    sdata2 = rep4.split('"')
    P=0
    Y=0
    Ya=0
    Yb=0
    while P < len(sdata2):
        if sdata2[P] == 'imgContainer':
            rdata2.append(str(sdata2[P+5]))
        elif sdata2[P] == 'Club':
            if Yb==0:
                rdata2.append(str(sdata2[P + 13]))
                Yb=Yb+1
            else:
                Yb=Yb
        elif sdata2[P] == 'Appearances':
            if Y==0:
                if Yb==0:
                    rdata2.append("")
                rdata2.append(str(sdata2[P + 7]))
                Y=Y+1
            else:
                Y=Y
        elif sdata2[P] == 'goals':
            if Ya==0:
                rdata2.append(str(sdata2[P + 3]))
                Ya=Ya+1
            else:
                Ya=Ya
        elif sdata2[P] == 'goal_assist':
            rdata2.append(str(sdata2[P + 3]))
        elif sdata2[P] == 'wins':
            rdata2.append(str(sdata2[P + 3]))
        elif sdata2[P] == 'losses':
            rdata2.append(str(sdata2[P + 3]))
        elif sdata2[P] == 'fouls':
            rdata2.append(str(sdata2[P + 3]))
        P = P+1

    j=j+1
print(rdata2)

