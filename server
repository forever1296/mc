#-*- coding: utf-8 -*-
#by forever1296
#패키지 import
import requests
from datetime import datetime
from bs4 import BeautifulSoup
import pymysql.cursors
#오늘 날짜
now_all = datetime.now()
now_want = now_all.strftime('%Y%m%d')
#https://sports.news.naver.com/wfootball/record/ajaxHtmlPlayerRecord.nhn?year=2019&month=12&category=primera&date=20191201
#https://sports.news.naver.com/wfootball/record/ajaxHtmlPlayerRecord.nhn?year=2019&month=12&category=epl&date=20191201

    # 해당 url에서 html을 불러옴.


url = requests.get('https://sports.news.naver.com/kfootball/record/index.nhn?category=kleague&year=2019')
urlc = url.content
html = str(BeautifulSoup(urlc, "html.parser"))
#data = str(html.find_all("script", {"type": "text/javascript"}))
#datat = str(html.find_all("script"))
rep1 = html.replace(':','')
rep2 = rep1.replace(',','')
rep1t = html.replace(':','')
rep2t = rep1t.replace(',','')
sdata = rep2.split('"')
sdatat = rep2t.split('"')

#print(datat)

rdata = list()
rdatat = list()
sdatasp = list()
i=0
j=1

conn = pymysql.connect(host='localhost',
        user='root',
        password='root',
        db='Sports',
        charset='utf8')

while i < len(sdata):
    if sdata[i] == 'rank':
        if sdata[i+1] == str(j):
            rdata.append(str(j) + '위')
            rdata.append(str(sdata[i - 36])) #
            rdata.append(str(sdata[i - 21])) #
            rdata.append(str(sdata[i - 29]))
            sdatasp = sdata[i + 5].split('}')
            rdata.append(str(sdatasp[0]))
            rdata.append(str(sdata[i - 7]))
            rdata.append(str(sdata[i - 23]))
            rdata.append(str(sdata[i - 33]))
            rdata.append(str(sdata[i - 11]))
            rdata.append(str(sdata[i - 1]))
            j = j + 1
        if j==7:
            i=0
    i = i + 1
    # 순위 팀명 승점 경기수 승 무 패 득점 실점 득실차
u=0
k=0
while u<len(rdata)/15-1:

    cursor = conn.cursor()
    a = rdata[k]
    b = rdata[k+1]
    c = rdata[k+2]
    d = rdata[k+3]
    e = rdata[k+4]
    f = rdata[k+5]
    g = rdata[k + 6]
    aa = rdata[k + 7]
    bb = rdata[k + 8]
    cc = rdata[k + 9]
    sql = "INSERT INTO kleaguerecordt(a,b,c,d,e,f,g,aa,bb,cc) VALUES ('"+ a +"','"+ b +"','"+ c +"','"+ d +"','"+ e +"','"+ f +"','"+ g +"','"+ aa +"','"+ bb +"','"+ cc +"')"
    k = k+10
    cursor.execute(sql)
    u = u + 1

i=0
j=0
while i < len(sdatat):
    if sdatat[i] == 'goalPerSt':
        rdatat.append(str(j) + '위')
        rdatat.append(str(sdatat[i + 34]))
        rdatat.append(str(sdatat[i - 4]))
        rdatat.append(str(sdatat[i - 1]))
        #sdatasp = sdata[i + 5].split('}')
        #rdata.append(str(sdatasp[0]))
        rdatat.append(str(sdatat[i + 55]))
        rdatat.append(str(sdatat[i + 23]))
        rdatat.append(str(sdatat[i + 11]))
        rdatat.append(str(sdatat[i + 53]))
        rdatat.append(str(sdatat[i + 25]))
        rdatat.append(str(sdatat[i + 46]))
        rdatat.append(str(sdatat[i + 37]))
        rdatat.append(str(sdatat[i + 5]))
        j = j + 1
    i = i + 1

#순위 선수이름 소속 득점 도움 공격포인트 슈팅 오프사이드 유효슈팅 평균평점 mvp bet11
jj=0
k=12
while jj<len(rdatat)/12-1:
    cursor = conn.cursor()
    a = rdatat[k]
    b = rdatat[k+1]
    c = rdatat[k+2]
    d = rdatat[k+3]
    e = rdatat[k+4]
    f = rdatat[k+5]
    g = rdatat[k+6]
    aa = rdatat[k+7]
    bb = rdatat[k+8]
    cc = rdatat[k+9]
    dd = rdatat[k+10]
    ee = rdatat[k+11]
    sql = "INSERT INTO kleaguerecord(a,b,c,d,e,f,g,aa,bb,cc,dd,ee) VALUES ('"+ a +"','"+ b +"','"+ c +"','"+ d +"','"+ e +"','"+ f +"','"+ g +"','"+ aa +"','"+ bb +"','"+ cc +"','"+ dd +"','"+ ee +"')"
    k = k+12
    cursor.execute(sql)
    jj = jj + 1
conn.commit()
conn.close()
#-*- coding: utf-8 -*-
#by forever1296
#패키지 import
import requests
from datetime import datetime
from bs4 import BeautifulSoup
import pymysql.cursors
#오늘 날짜
now_all = datetime.now()
now_want = now_all.strftime('%Y%m%d')
#https://sports.news.naver.com/wfootball/record/ajaxHtmlPlayerRecord.nhn?year=2019&month=12&category=primera&date=20191201
#https://sports.news.naver.com/wfootball/record/ajaxHtmlPlayerRecord.nhn?year=2019&month=12&category=epl&date=20191201

    # 해당 url에서 html을 불러옴.


url = requests.get('https://sports.news.naver.com/kfootball/record/index.nhn?category=kleague2&year=2019')
urlc = url.content
html = str(BeautifulSoup(urlc, "html.parser"))
#data = str(html.find_all("script", {"type": "text/javascript"}))
#datat = str(html.find_all("script"))
rep1 = html.replace(':','')
rep2 = rep1.replace(',','')
rep1t = htmlt.replace(':','')
rep2t = rep1t.replace(',','')
sdata = rep2.split('"')
sdatat = rep2t.split('"')

#print(datat)

rdata = list()
rdatat = list()
sdatasp = list()
i=0
j=1

conn = pymysql.connect(host='localhost',
        user='root',
        password='root',
        db='Sports',
        charset='utf8')

while i < len(sdata):
    if sdata[i] == 'rank':
        if sdata[i+1] == str(j):
            rdata.append(str(j) + '위')
            rdata.append(str(sdata[i - 36])) #
            rdata.append(str(sdata[i - 21])) #
            rdata.append(str(sdata[i - 29]))
            sdatasp = sdata[i + 5].split('}')
            rdata.append(str(sdatasp[0]))
            rdata.append(str(sdata[i - 7]))
            rdata.append(str(sdata[i - 23]))
            rdata.append(str(sdata[i - 33]))
            rdata.append(str(sdata[i - 11]))
            rdata.append(str(sdata[i - 1]))
            j = j + 1
    i = i + 1
    # 순위 팀명 승점 경기수 승 무 패 득점 실점 득실차
u=0
k=0
while u<len(rdata)/20:

    cursor = conn.cursor()
    a = rdata[k]
    b = rdata[k+1]
    c = rdata[k+2]
    d = rdata[k+3]
    e = rdata[k+4]
    f = rdata[k+5]
    g = rdata[k + 6]
    aa = rdata[k + 7]
    bb = rdata[k + 8]
    cc = rdata[k + 9]
    sql = "INSERT INTO kleague2recordt(a,b,c,d,e,f,g,aa,bb,cc) VALUES ('"+ a +"','"+ b +"','"+ c +"','"+ d +"','"+ e +"','"+ f +"','"+ g +"','"+ aa +"','"+ bb +"','"+ cc +"')"
    k = k+10
    cursor.execute(sql)
    u = u + 1

i=0
j=0
while i < len(sdatat):
    if sdatat[i] == 'goalPerSt':
        rdatat.append(str(j) + '위')
        rdatat.append(str(sdatat[i + 34]))
        rdatat.append(str(sdatat[i - 4]))
        rdatat.append(str(sdatat[i - 1]))
        #sdatasp = sdata[i + 5].split('}')
        #rdata.append(str(sdatasp[0]))
        rdatat.append(str(sdatat[i + 55]))
        rdatat.append(str(sdatat[i + 23]))
        rdatat.append(str(sdatat[i + 11]))
        rdatat.append(str(sdatat[i + 53]))
        rdatat.append(str(sdatat[i + 25]))
        rdatat.append(str(sdatat[i + 46]))
        rdatat.append(str(sdatat[i + 37]))
        rdatat.append(str(sdatat[i + 5]))
        j = j + 1
    i = i + 1

#순위 선수이름 소속 득점 도움 공격포인트 슈팅 오프사이드 유효슈팅 평균평점 mvp bet11
jj=0
k=12
while jj<len(rdatat)/12-1:
    cursor = conn.cursor()
    a = rdatat[k]
    b = rdatat[k+1]
    c = rdatat[k+2]
    d = rdatat[k+3]
    e = rdatat[k+4]
    f = rdatat[k+5]
    g = rdatat[k+6]
    aa = rdatat[k+7]
    bb = rdatat[k+8]
    cc = rdatat[k+9]
    dd = rdatat[k+10]
    ee = rdatat[k+11]
    sql = "INSERT INTO kleague2record(a,b,c,d,e,f,g,aa,bb,cc,dd,ee) VALUES ('"+ a +"','"+ b +"','"+ c +"','"+ d +"','"+ e +"','"+ f +"','"+ g +"','"+ aa +"','"+ bb +"','"+ cc +"','"+ dd +"','"+ ee +"')"
    k = k+12
    cursor.execute(sql)
    jj = jj + 1
conn.commit()
conn.close()
